# ~/.bash_aliases: executed by bash(1) for non-login shells.

# some ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

# cd aliases
alias ..='cd ..'

# vim alias
alias vi='vim'
